{
  "name": "@random-ru/bot",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn build:dev && concurrently 'yarn:dev:*' -c auto -k",
    "dev:build": "NODE_ENV=development webpack --watch",
    "dev:start": "NODE_ENV=development nodemon dist/main.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "build": "yarn generate && NODE_ENV=production webpack",
    "start": "yarn migrate && NODE_ENV=production node dist/main.js",
    "build:dev": "NODE_ENV=development webpack",
    "start:dev": "NODE_ENV=development nodemon dist/main.js --inspect",
    "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,mts,jsx,tsx}\"",
    "lint:fix": "yarn lint --fix"
  },
  "volta": {
    "node": "20.10.0"
  },
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "grammy": "^1.19.2",
    "wretch": "^2.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/node": "^20.10.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-node-externals": "^3.0.4",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "eslint-kit": "^10.6.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
